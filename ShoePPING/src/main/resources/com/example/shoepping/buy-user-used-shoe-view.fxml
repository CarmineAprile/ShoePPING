                  <MenuItem fx:id="itemSize41" mnemonicParsing="false" onAction="#size41" style="-fx-text-fill: #554b4d;" text="41" />
                  <MenuItem fx:id="itemSize42" mnemonicParsing="false" onAction="#size42" style="-fx-text-fill: #554b4d;" text="42" />
                  <MenuItem fx:id="itemSize43" mnemonicParsing="false" onAction="#size43" style="-fx-text-fill: #554b4d;" text="43" />
                  <MenuItem fx:id="itemSize44" mnemonicParsing="false" onAction="#size44" style="-fx-text-fill: #554b4d;" text="44" />
                  <MenuItem fx:id="itemSize45" mnemonicParsing="false" onAction="#size45" style="-fx-text-fill: #554b4d;" text="45" />
                  <MenuItem fx:id="itemSize46" mnemonicParsing="false" onAction="#size46" style="-fx-text-fill: #554b4d;" text="46" />
               </items>
               <VBox.margin>
                  <Insets left="20.0" right="10.0" top="20.0" />
               </VBox.margin>
            </MenuButton>
            <TextField fx:id="priceFilter" focusTraversable="false" layoutX="10.0" layoutY="37.0" promptText="Select max price">
               <VBox.margin>
                  <Insets left="20.0" right="10.0" top="20.0" />
               </VBox.margin>
            </TextField>
            <Label fx:id="maxPriceL" prefHeight="17.0" prefWidth="291.0" textFill="RED">
               <VBox.margin>
                  <Insets left="20.0" right="10.0" top="5.0" />
               </VBox.margin>
            </Label>
            <Button fx:id="applyFilter" focusTraversable="false" mnemonicParsing="false" onAction="#apply" prefHeight="25.0" prefWidth="367.0" style="-fx-background-color: #F1CDD4; -fx-background-radius: 20;" text="APPLY" textFill="#554b4d">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
               <VBox.margin>
                  <Insets left="20.0" right="10.0" top="5.0" />
               </VBox.margin>
            </Button>
         </VBox>
         <VBox prefHeight="632.0" prefWidth="450.0">
            <Label alignment="TOP_CENTER" prefHeight="27.0" prefWidth="499.0" text="Shoe catalog" textFill="#554b4d">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
               <VBox.margin>
                  <Insets left="10.0" right="10.0" />
               </VBox.margin>
            </Label>
            <ScrollPane fitToWidth="true" hbarPolicy="NEVER" prefHeight="445.0" prefWidth="170.0">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
               <VBox fx:id="vBoxCatalog" prefHeight="356.0" prefWidth="428.0" />
            </ScrollPane>
         </VBox>
      </HBox>
   </VBox>
</AnchorPane>
