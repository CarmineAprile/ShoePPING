                  <MenuItem fx:id="itemSize38" mnemonicParsing="false" onAction="#size38" style="-fx-text-fill: #554b4d;" text="38" />
                  <MenuItem fx:id="itemSize39" mnemonicParsing="false" onAction="#size39" style="-fx-text-fill: #554b4d;" text="39" />
